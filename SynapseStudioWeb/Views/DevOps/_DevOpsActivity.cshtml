@model List<SynapseStudioWeb.Models.DatabaseActivityDto>
@{
    Layout = null;
}

<div class="col-md-12">
    <table class="table">
        <thead>
            <tr>
                <th>PID</th>
                <th>User</th>
                <th width="20%">Application</th>
                <th>Client Addr.</th>
                <th>Query</th>
                <th width="15%">Query Start</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var m in Model)
            {
                <tr>
                    <td>@m.pid</td>
                    <td>@m.usename</td>
                    <td>@m.application_name</td>
                    <td>@m.client_addr</td>
                    <td>@m.query</td>
                    <td>@m.query_start</td>
                </tr>
            }
            @if (Model.Count == 0)
            {
                <tr>
                    <td colspan="5">
                        No record found
                    <td>
                </tr>
            }
        </tbody>
    </table>
</div>

